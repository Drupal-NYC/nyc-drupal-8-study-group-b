<?php
/**
 * @file acme_session.module
 *
 * Contains hook implementations for acme_session.
 */

/**
 * Implements hook_theme().
 */
function acme_session_theme($existing, $type, $theme, $path) {
  return [
    'views_view_unformatted__acme_sessions' => [
      'path' => $path . '/templates',
      'base hook' => 'views_view_unformatted',
      'variables' => [
        'view' => NULL,
        'options' => NULL,
        'rows' => NULL,
        'title' => '',
       ],
      'preprocess functions' => [
        'template_preprocess',
        'template_preprocess_views_view_unformatted',
      ],
    ]
  ];
}

