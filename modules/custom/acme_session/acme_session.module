/**
 * Implements hook_theme().
 */
function acme_session_theme($existing, $type, $theme, $path) {
  return [
    'views_view_unformatted__acme_session' => [
      'path' => $path . '/templates',
      'file' => 'views-view-unformatted--acme-session.html.twig',
      'base hook' => 'views_view',
      'arguments' => [
        'view' => NULL,
        'options' => NULL,
        'rows' => NULL,
        'title' => '',
       ],
      'preprocess functions' => array(
        'template_preprocess',
        'template_preprocess_views_view_unformatted',
      ),
    ]
  ];
}

